generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  username String  @unique
  password String
  admin    Boolean
}

model Test {
  id   Int    @id @default(autoincrement())
  text String @unique
}
